<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mingun.trainerinmyhand"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="mingun.trainerinmyhand.TrainerInMyHand"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mingun.trainerinmyhand.Planning"
            android:label="@string/title_activity_planning" >
        </activity>
        <activity
            android:name="mingun.trainerinmyhand.FoodInput"
            android:label="@string/title_activity_food_input" >
        </activity>
        <activity
            android:name="mingun.trainerinmyhand.FoodAdd"
            android:label="@string/title_activity_food_add" >
        </activity>
    </application>

</manifest>